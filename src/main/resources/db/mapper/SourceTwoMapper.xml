<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tkurek.wat.Etl.mapper.SourceTwoMapper">

    <select id="selectAllLocality" resultType="com.tkurek.wat.Etl.model.sourceTwo.SourceTwo_Locality">
        select * from locality;
    </select>

    <select id="selectAllRegion" resultType="com.tkurek.wat.Etl.model.sourceTwo.SourceTwo_Region">
        select * from region;
    </select>

    <select id="selectAllShop" resultType="com.tkurek.wat.Etl.model.sourceTwo.SourceTwo_Shop">
        select * from shop;
    </select>

    <select id="selectAllTypeWorker" resultType="com.tkurek.wat.Etl.model.sourceTwo.SourceTwo_TypeWorker">
        select * from typeWorker;
    </select>

    <select id="selectAllWorker" resultType="com.tkurek.wat.Etl.model.sourceTwo.SourceTwo_Worker">
        select * from worker;
    </select>

</mapper>